{
    "name": "syllabus",
    "version": "0.1.0",
    "private": true,
    "repository": "https://github.com/olexu/syllabus",
    "author": "Oleksandr Ulianytskyi",
    "scripts": {
        "start": "rimraf ./build && react-scripts start",
        "build": "react-scripts build && copy-files-from-to",
        "test": "cross-env CI=true react-scripts test",
        "eject": "react-scripts eject",
        "lint": "eslint \"**/*.ts\" \"**/*.tsx\""
    },
    "dependencies": {
        "bootstrap": "^4.4.1",
        "jquery": "^3.4.1",
        "node-sass": "^4.13.1",
        "react": "^16.13.0",
        "react-bootstrap": "^1.0.0-beta.17",
        "react-dom": "^16.13.0",
        "react-router": "^5.1.2",
        "react-router-dom": "^5.1.2",
        "react-scripts": "3.4.0",
        "typescript": "~3.7.2"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^4.2.4",
        "@testing-library/react": "^9.3.2",
        "@testing-library/user-event": "^7.1.2",
        "@types/jest": "^24.0.0",
        "@types/node": "^12.0.0",
        "@types/react": "^16.9.0",
        "@types/react-dom": "^16.9.0",
        "ajv": "^6.12.0",
        "copy-files-from-to": "^2.3.0",
        "cross-env": "^5.2.0",
        "eslint-plugin-prettier": "^3.1.2",
        "prettier": "^1.19.1",
        "rimraf": "^3.0.2"
    },
    "eslintConfig": {
        "extends": "react-app",
        "plugins": [
            "prettier"
        ],
        "rules": {
            "prettier/prettier": "error"
        }
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all",
            "ie 11"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
